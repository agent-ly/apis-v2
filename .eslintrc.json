{
  "$schema": "http://json.schemastore.org/eslintrc",
  "env": {
    "node": true
  },
  "extends": ["prettier"],
  "parser": "@typescript-eslint/parser",
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module"
  },
  "plugins": ["@typescript-eslint"],
  "rules": {
    "curly": ["warn", "all"]
  }
}
